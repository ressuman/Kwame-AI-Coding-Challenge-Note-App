{
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    {
      "dest": "src/server.js",
      "src": "/(.*)"
    }
  ],
  "version": 2
}
